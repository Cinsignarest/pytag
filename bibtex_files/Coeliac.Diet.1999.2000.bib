@article{
   author = {Siry, M. and Burges, C. and Stiens, R. and Schneider, H. and Steiff, J.},
   title = {[First diagnosis of celiac disease in a 67-year old female patient]},
   journal = {Dtsch Med Wochenschr},
   volume = {125},
