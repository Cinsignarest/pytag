@article{
   author = {Dorum, S. and Qiao, S. W. and Sollid, L. M. and Fleckenstein, B.},
   title = {A quantitative analysis of transglutaminase 2-mediated deamidation of gluten peptides: implications for the T-cell response in celiac disease},
   journal = {J Proteome Res},
   volume = {8},
