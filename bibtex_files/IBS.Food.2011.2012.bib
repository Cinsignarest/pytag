   author = {Bailon, E. and Cueto-Sola, M. and Utrilla, P. and Nieto, A. and Garrido-Mesa, N. and Celada, A. and Zarzuelo, A. and Xaus, J. and Galvez, J. and Comalada, M.},
   title = {DNFB-DNS hapten-induced colitis in mice should not be considered a model of inflammatory bowel disease},
   journal = {Inflamm Bowel Dis},
   volume = {17},
   number = {10},
