@article{
   author = {Murakami, K. and Habukawa, C. and Nobuta, Y. and Moriguchi, N. and Takemura, T.},
   title = {The effect of Lactobacillus brevis KB290 against irritable bowel syndrome: a placebo-controlled double-blind crossover trial},
   journal = {Biopsychosoc Med},
