   author = {Bai, J. and Khaldi, S. and Gargala, G. and Baishanbo, A. and Francois, A. and Ballet, J. J. and Ducrotte, P. and Fioramonti, J. and Favennec, L. and Le Goff, L.},
   title = {Effects of octreotide on jejunal hypersensitivity triggered by Cryptosporidium parvum intestinal infection in an immunocompetent suckling rat model},
