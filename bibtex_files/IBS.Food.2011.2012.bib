@article{
   author = {Barbara, G. and Zecchi, L. and Barbaro, R. and Cremon, C. and Bellacosa, L. and Marcellini, M. and De Giorgio, R. and Corinaldesi, R. and Stanghellini, V.},
   title = {Mucosal permeability and immune activation as potential therapeutic targets of probiotics in irritable bowel syndrome},
   journal = {J Clin Gastroenterol},
