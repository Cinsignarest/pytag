@article{
   author = {Barlow, J. M. and Johnson, C. D. and Stephens, D. H.},
   title = {Celiac disease: how common is jejunoileal fold pattern reversal found at small-bowel follow-through?},
   journal = {AJR Am J Roentgenol},
   volume = {166},
   number = {3},
