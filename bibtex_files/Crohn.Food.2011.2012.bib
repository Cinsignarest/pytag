@article{
   author = {Lichtenstein, G. R. and Hanauer, S. B. and Sandborn, W. J.},
   title = {Risk of Biologic Therapy-Associated Progressive Multifocal Leukoencephalopathy: Use of the JC Virus Antibody Assay in the Treatment of Moderate-to-Severe Crohn's Disease},
   journal = {Gastroenterol Hepatol (N Y)},
   volume = {8},
