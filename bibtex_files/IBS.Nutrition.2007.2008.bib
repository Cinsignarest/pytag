@article{
   author = {Walker, T. R. and Land, M. L. and Kartashov, A. and Saslowsky, T. M. and Lyerly, D. M. and Boone, J. H. and Rufo, P. A.},
   title = {Fecal lactoferrin is a sensitive and specific marker of disease activity in children and young adults with inflammatory bowel disease},
   journal = {J Pediatr Gastroenterol Nutr},
