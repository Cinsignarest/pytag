   author = {Roka, R. and Rosztoczy, A. and Leveque, M. and Izbeki, F. and Nagy, F. and Molnar, T. and Lonovics, J. and Garcia-Villar, R. and Fioramonti, J. and Wittmann, T. and Bueno, L.},
