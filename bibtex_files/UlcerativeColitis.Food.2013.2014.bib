@article{
   author = {Yamada, S. and Koyama, T. and Noguchi, H. and Ueda, Y. and Kitsuyama, R. and Shimizu, H. and Tanimoto, A. and Wang, K. Y. and Nawata, A. and Nakayama, T. and Sasaguri, Y. and Satoh, T.},
