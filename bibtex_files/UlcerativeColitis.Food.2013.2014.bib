   author = {Bellavia, M. and Tomasello, G. and Romeo, M. and Damiani, P. and Lo Monte, A. I. and Lozio, L. and Campanella, C. and Marino Gammazza, A. and Rappa, F. and Zummo, G. and Cocchi, M. and Conway de Macario, E. and Macario, A. J. and Cappello, F.},
