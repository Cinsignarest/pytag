@article{
   author = {Deepak, P. and Sifuentes, H. and Sherid, M. and Stobaugh, D. and Sadozai, Y. and Ehrenpreis, E. D.},
   title = {T-cell non-Hodgkin's lymphomas reported to the FDA AERS with tumor necrosis factor-alpha (TNF-alpha) inhibitors: results of the REFURBISH study},
