@article{
   author = {Lamouse-Smith, E. S. and Weber, S. and Rossi, R. F. and Neinstedt, L. J. and Mosammaparast, N. and Sandora, T. J. and McAdam, A. J. and Bousvaros, A.},
   title = {Polymerase chain reaction test for Clostridium difficile toxin B gene reveals similar prevalence rates in children with and without inflammatory bowel disease},
   journal = {J Pediatr Gastroenterol Nutr},
   volume = {57},
