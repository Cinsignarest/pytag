@article{
   author = {Ainley, C. and Cason, J. and Slavin, B. M. and Wolstencroft, R. A. and Thompson, R. P.},
   title = {The influence of zinc status and malnutrition on immunological function in Crohn's disease},
   journal = {Gastroenterology},
   volume = {100},
