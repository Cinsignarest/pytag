@article{
   author = {Halpert, A. and Dalton, C. B. and Palsson, O. and Morris, C. and Hu, Y. and Bangdiwala, S. and Hankins, J. and Norton, N. and Drossman, D.},
   title = {What patients know about irritable bowel syndrome (IBS) and what they would like to know. National Survey on Patient Educational Needs in IBS and development and validation of the Patient Educational Needs Questionnaire (PEQ)},
   journal = {Am J Gastroenterol},
   volume = {102},
   number = {9},
   pages = {1972-82},
   note = {Halpert, Albena
Dalton, Christine B
Palsson, Olafur
Morris, Carolyn
