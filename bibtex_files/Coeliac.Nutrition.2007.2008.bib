@article{
   author = {Koskinen, O. and Collin, P. and Korponay-Szabo, I. and Salmi, T. and Iltanen, S. and Haimila, K. and Partanen, J. and Maki, M. and Kaukinen, K.},
   title = {Gluten-dependent small bowel mucosal transglutaminase 2-specific IgA deposits in overt and mild enteropathy coeliac disease},
