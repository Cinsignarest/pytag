@article{
   author = {Padmanabhan, P. and Grosse, J. and Asad, A. B. and Radda, G. K. and Golay, X.},
   title = {Gastrointestinal transit measurements in mice with 99mTc-DTPA-labeled activated charcoal using NanoSPECT-CT},
   journal = {EJNMMI Res},
   volume = {3},
