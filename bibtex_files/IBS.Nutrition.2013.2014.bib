@article{
   author = {Posserud, I. and Strid, H. and Storsrud, S. and Tornblom, H. and Svensson, U. and Tack, J. and Van Oudenhove, L. and Simren, M.},
   title = {Symptom pattern following a meal challenge test in patients with irritable bowel syndrome and healthy controls},
   journal = {United European Gastroenterol J},
