@article{
   author = {Sanchez de Medina, F. and Romero-Calvo, I. and Mascaraque, C. and Martinez-Augustin, O.},
   title = {Intestinal inflammation and mucosal barrier function},
   journal = {Inflamm Bowel Dis},
   volume = {20},
