@article{
   author = {Lebrero-Fernandez, C. and Wenzel, U. A. and Akeus, P. and Wang, Y. and Strid, H. and Simren, M. and Gustavsson, B. and Borjesson, L. G. and Cardell, S. L. and Ohman, L. and Quiding-Jarbrink, M. and Bas-Forsberg, A.},
   title = {Altered expression of Butyrophilin (BTN) and BTN-like (BTNL) genes in intestinal inflammation and colon cancer},
