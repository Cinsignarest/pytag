@article{
   author = {Heuschkel, R. and Salvestrini, C. and Beattie, R. M. and Hildebrand, H. and Walters, T. and Griffiths, A.},
   title = {Guidelines for the management of growth failure in childhood inflammatory bowel disease},
   journal = {Inflamm Bowel Dis},
   volume = {14},
