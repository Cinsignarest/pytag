@article{
   author = {Benchimol, E. I. and Seow, C. H. and Steinhart, A. H. and Griffiths, A. M.},
   title = {Traditional corticosteroids for induction of remission in Crohn's disease},
   journal = {Cochrane Database Syst Rev},
   number = {2},
