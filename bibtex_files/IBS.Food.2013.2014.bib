@article{
   author = {Tsilingiri, K. and Sonzogni, A. and Caprioli, F. and Rescigno, M.},
   title = {A novel method for the culture and polarized stimulation of human intestinal mucosa explants},
   journal = {J Vis Exp},
   number = {75},
