@article{
   author = {Vahedi, K. and Mascart, F. and Mary, J. Y. and Laberenne, J. E. and Bouhnik, Y. and Morin, M. C. and Ocmant, A. and Velly, C. and Colombel, J. F. and Matuchansky, C.},
