@article{
   author = {Tsertsvadze, A. and Gurung, T. and Court, R. and Clarke, A. and Sutcliffe, P.},
   title = {Clinical effectiveness and cost-effectiveness of elemental nutrition for the maintenance of remission in Crohn's disease: a systematic review and meta-analysis},
   journal = {Health Technol Assess},
