@article{
   author = {Suh, J. H. and Saba, J. D.},
   title = {Sphingosine-1-phosphate in inflammatory bowel disease and colitis-associated colon cancer: the fat's in the fire},
   journal = {Transl Cancer Res},
   volume = {4},
