@article{
   author = {Suskind, D. L. and Cohen, S. A. and Brittnacher, M. J. and Wahbeh, G. and Lee, D. and Shaffer, M. L. and Braly, K. and Hayden, H. S. and Klein, J. and Gold, B. and Giefer, M. and Stallworth, A. and Miller, S. I.},
