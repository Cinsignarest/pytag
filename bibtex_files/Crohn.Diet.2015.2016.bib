@article{
   author = {Akobeng, A. K. and Elawad, M. and Gordon, M.},
   title = {Glutamine for induction of remission in Crohn's disease},
   journal = {Cochrane Database Syst Rev},
   volume = {2},
   pages = {Cd007348},
   note = {1469-493x
