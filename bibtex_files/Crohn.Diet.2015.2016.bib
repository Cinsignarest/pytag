@article{
   author = {Stratton, L. and Caddy, G. R.},
   title = {Listeria Rhombencephalitis Complicating Anti-TNF Treatment during an Acute Flare of Crohn's Colitis},
   journal = {Case Rep Gastrointest Med},
   volume = {2016},
   pages = {6216128},
   note = {Stratton, L
