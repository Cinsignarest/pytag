@article{
   author = {Abd El-Atti, S. and Wasicek, K. and Mark, S. and Hegazi, R.},
   title = {Use of probiotics in the management of chemotherapy-induced diarrhea: a case study},
   journal = {JPEN J Parenter Enteral Nutr},
   volume = {33},
   number = {5},
   pages = {569-70},
   note = {Abd El-Atti, Samia
Wasicek, Kelley
