@article{
   author = {Barouei, J. and Adams, M. C. and Hodgson, D. M.},
   title = {Prophylactic role of maternal administration of probiotics in the prevention of irritable bowel syndrome},
   journal = {Med Hypotheses},
   volume = {73},
   number = {5},
   pages = {764-7},
   note = {1532-2777
