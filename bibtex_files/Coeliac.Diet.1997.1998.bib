@article{
   author = {Zimmer, K. P. and Naim, H. and Weber, P. and Ellis, H. J. and Ciclitira, P. J.},
   title = {Targeting of gliadin peptides, CD8, alpha/beta-TCR, and gamma/delta-TCR to Golgi complexes and vacuoles within celiac disease enterocytes},
   journal = {Faseb j},
