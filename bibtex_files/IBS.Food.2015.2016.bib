@article{
   author = {Bennett, W. E., Jr.},
   title = {Quantitative Risk-Benefit Analysis of Probiotic Use for Irritable Bowel Syndrome and Inflammatory Bowel Disease},
   journal = {Drug Saf},
   volume = {39},
   number = {4},
