@article{
   author = {Tang, L. and Cheng, C. Y. and Sun, X. and Pedicone, A. J. and Mohamadzadeh, M. and Cheng, S. X.},
   title = {The Extracellular Calcium-Sensing Receptor in the Intestine: Evidence for Regulation of Colonic Absorption, Secretion, Motility, and Immunity},
   journal = {Front Physiol},
   volume = {7},
   pages = {245},
   note = {Tang, Lieqi
