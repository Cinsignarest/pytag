@article{
   author = {Barera, G. and Bazzigaluppi, E. and Viscardi, M. and Renzetti, F. and Bianchi, C. and Chiumello, G. and Bosi, E.},
   title = {Macroamylasemia attributable to gluten-related amylase autoantibodies: a case report},
   journal = {Pediatrics},
   volume = {107},
   number = {6},
   pages = {E93},
   note = {1098-4275
Barera, G
Bazzigaluppi, E
Viscardi, M
Renzetti, F
Bianchi, C
