@article{
   author = {Rashid, M. and Cranney, A. and Zarkadas, M. and Graham, I. D. and Switzer, C. and Case, S. and Molloy, M. and Warren, R. E. and Burrows, V. and Butzner, J. D.},
   title = {Celiac disease: evaluation of the diagnosis and dietary compliance in Canadian children},
