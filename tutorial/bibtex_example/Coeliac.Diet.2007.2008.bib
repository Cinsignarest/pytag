@article{
   author = {Aziz, S. and Muzaffar, R. and Zafar, M. N. and Mehnaz, A. and Mubarak, M. and Abbas, Z. and Naqvi, S. A. and Rizvi, A. H.},
   title = {Celiac disease in children with persistent diarrhea and failure to thrive},
