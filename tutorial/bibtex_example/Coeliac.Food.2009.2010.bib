@article{
   author = {Zimmer, K. P. and Fischer, I. and Mothes, T. and Weissen-Plenz, G. and Schmitz, M. and Wieser, H. and Buning, J. and Lerch, M. M. and Ciclitira, P. C. and Weber, P. and Naim, H. Y.},
