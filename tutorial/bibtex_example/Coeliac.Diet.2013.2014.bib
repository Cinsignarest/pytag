@article{
   author = {Carroccio, A. and Mansueto, P. and D'Alcamo, A. and Iacono, G.},
   title = {Non-celiac wheat sensitivity as an allergic condition: personal experience and narrative review},
   journal = {Am J Gastroenterol},
   volume = {108},
   number = {12},
   pages = {1845-52; quiz 1853},
   note = {1572-0241
