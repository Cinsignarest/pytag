@article{
   author = {Adriaanse, M. P. and Tack, G. J. and Passos, V. L. and Damoiseaux, J. G. and Schreurs, M. W. and van Wijck, K. and Riedl, R. G. and Masclee, A. A. and Buurman, W. A. and Mulder, C. J. and Vreugdenhil, A. C.},
