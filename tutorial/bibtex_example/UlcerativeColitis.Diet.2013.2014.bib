@article{
   author = {Wang, L. S. and Kuo, C. T. and Stoner, K. and Yearsley, M. and Oshima, K. and Yu, J. and Huang, T. H. and Rosenberg, D. and Peiffer, D. and Stoner, G. and Huang, Y. W.},
   title = {Dietary black raspberries modulate DNA methylation in dextran sodium sulfate (DSS)-induced ulcerative colitis},
   journal = {Carcinogenesis},
