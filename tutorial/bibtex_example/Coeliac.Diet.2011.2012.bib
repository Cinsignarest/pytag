@article{
   author = {Tanpowpong, P. and Broder-Fingert, S. and Katz, A. J. and Camargo, C. A., Jr.},
   title = {Age-related patterns in clinical presentations and gluten-related issues among children and adolescents with celiac disease},
   journal = {Clin Transl Gastroenterol},
   volume = {3},
   pages = {e9},
   note = {Tanpowpong, Pornthep
Broder-Fingert, Sarabeth
Katz, Aubrey J
