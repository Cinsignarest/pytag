   author = {McNally, S. L. and Donohue, M. C. and Newton, K. P. and Ogletree, S. P. and Conner, K. K. and Ingegneri, S. E. and Kagnoff, M. F.},
   title = {Can consumers trust web-based information about celiac disease? Accuracy, comprehensiveness, transparency, and readability of information on the internet},
   journal = {Interact J Med Res},
   volume = {1},
   number = {1},
   pages = {e1},
   note = {McNally, Shawna L
