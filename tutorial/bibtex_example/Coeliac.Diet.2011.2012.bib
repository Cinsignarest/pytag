@article{
   author = {Lidums, I. and Teo, E. and Field, J. and Cummins, A. G.},
   title = {Capsule endoscopy: a valuable tool in the follow-up of people with celiac disease on a gluten-free diet},
   journal = {Clin Transl Gastroenterol},
   volume = {2},
   pages = {e4},
   note = {Lidums, Ilmars
Teo, Edward
Field, John
