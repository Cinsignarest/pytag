   title = {TNF-alpha -308 G > A (rs1800629) Polymorphism is Associated with Celiac Disease: A Meta-analysis of 11 Case-Control Studies},
   journal = {Sci Rep},
   volume = {6},
   pages = {32677},
   note = {2045-2322
Khan, Saif
Mandal, Raju K
