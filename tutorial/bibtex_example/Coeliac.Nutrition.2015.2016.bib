@article{
   author = {van Leeuwen, M. A. and Costes, L. M. M. and van Berkel, L. A. and Simons-Oosterhuis, Y. and du Pre, M. F. and Kozijn, A. E. and Raatgeep, H. C. and Lindenbergh-Kortleve, D. J. and van Rooijen, N. and Koning, F. and Samsom, J. N.},
   title = {Macrophage-mediated gliadin degradation and concomitant IL-27 production drive IL-10- and IFN-gamma-secreting Tr1-like-cell differentiation in a murine model for gluten tolerance},
   journal = {Mucosal Immunol},
   volume = {10},
   number = {3},
   pages = {635-649},
   note = {1935-3456
