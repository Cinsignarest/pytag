@article{
   author = {Shi, L. and Lin, Q. and Yang, T. and Nie, Y. and Li, X. and Liu, B. and Shen, J. and Liang, Y. and Tang, Y. and Luo, F.},
   title = {Oral administration of Lentinus edodes beta-glucans ameliorates DSS-induced ulcerative colitis in mice via MAPK-Elk-1 and MAPK-PPARgamma pathways},
   journal = {Food Funct},
   volume = {7},
