@article{
   author = {Dugum, M. and Lin, J. and Lopez, R. and Estfan, B. and Manilich, E. and Stocchi, L. and Shen, B. and Liu, X.},
   title = {Recurrence and survival rates of inflammatory bowel disease-associated colorectal cancer following postoperative chemotherapy: a comparative study},
   journal = {Gastroenterol Rep (Oxf)},
   note = {Dugum, Mohannad
Lin, Jingmei
Lopez, Rocio
Estfan, Bassam
