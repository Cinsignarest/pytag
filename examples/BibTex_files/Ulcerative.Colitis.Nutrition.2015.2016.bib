@article{
   author = {Aloi, M. and Birimberg-Schwartz, L. and Buderus, S. and Hojsak, I. and Fell, J. M. and Bronsky, J. and Veereman, G. and Koletzko, S. and Shaoul, R. and Miele, E. and Turner, D. and Russell, R. K.},
   title = {Treatment Options and Outcomes of Pediatric IBDU Compared with Other IBD Subtypes: A Retrospective Multicenter Study from the IBD Porto Group of ESPGHAN},
   journal = {Inflamm Bowel Dis},
   volume = {22},
   number = {6},
   pages = {1378-83},
   note = {1536-4844
Aloi, Marina
