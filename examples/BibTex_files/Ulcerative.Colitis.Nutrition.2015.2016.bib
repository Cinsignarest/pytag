@article{
   author = {Stecher, B.},
   title = {The Roles of Inflammation, Nutrient Availability and the Commensal Microbiota in Enteric Pathogen Infection},
   journal = {Microbiol Spectr},
   volume = {3},
   number = {3},
   note = {2165-0497
Stecher, Barbel
Journal Article
